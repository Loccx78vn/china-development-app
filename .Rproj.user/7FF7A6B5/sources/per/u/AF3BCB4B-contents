selectYear_ui = selectInput("year", 
                            "Select Year", 
                            choices = c(sort(unique(china_gdp_sf$Year))), 
                            selected = min(unique(china_gdp_sf$Year)))

selectCompany_ui = selectInput(
  "company",
  "Select a Shipping Company:",
  choices = c("All",unname(COMPANIES$names)),
  selected = "All"
)
selectYear_ui
selectCompany_ui
line$ui("line")
map$ui("map")
onl_map$ui("onl_map")
stock$ui("stock")
table$ui("table")